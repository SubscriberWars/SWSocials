        window.setPage = function (body) {
          /*
          * just so i can remember these
          text: textcolor.value,
                background: backgroundcolor.value,
                box: boxcolor.value,
                boxwidth: boxwidth.value,
                mainodosize: mainodosize.value,
                averages: averages.checked,
                graph: graph.checked,
                banner: banner.checked,
                image: image.checked,
                name: name.checked,
                roundimage: roundimage.checked
          */
          const json = JSON.parse(body)
          console.log(json.text,json.background,json.box,json.boxwidth,json.mainodosize,json.averages,json.graph,json.banner,json.image,json.name,json.icon,json.roundimage)
          var oa = {}
          if (!json.text || json.text == "#010101") { } else {
            lestylehere.innerHTML += "* {color:" + json.text + " !important;}"
            oa.text = json.text
            textcolor.value = json.text
          }
          if (!json.background || json.background == "#010101") { } else {
            lestylehere.innerHTML += "body {background-color:" + json.background + " !important;}"
            oa.background = json.background
            backgroundcolor.value = json.background
          }
          if (!json.box || json.box == "#010101") { } else {
            lestylehere.innerHTML += ".modal-content, #disqus_thread, #disqus_recommendations, #embedbox, .btn, .searchbox, .searchbox2 {background-color:" + json.box + " !important;}"/*hm*/
            lestylehere.innerHTML += ".modal-content, #disqus_thread, #disqus_recommendations, #embedbox, .btn, .searchbox, .searchbox2 {border: 1px solid " + json.box + " !important;}"/*hm*/
            oa.box = json.box
            boxcolor.value = json.box
            setTimeout(() => {
            gengraph()
            }, 1000);
          }
          if (!json.boxwidth || json.boxwidth == "") { } else {
            lestylehere.innerHTML += ".modal-content, #disqus_thread, #disqus_recommendations, #embedbox, .searchbox,.smallers {width:" + json.boxwidth + " !important;}"/*hm*/
            oa.boxwidth = json.boxwidth
            boxwidth.value = json.boxwidth
          }
          if (!json.mainodosize || json.mainodosize == "") { } else {
            lestylehere.innerHTML += "#mainodo {font-size:" + json.mainodosize + " !important;}"/*hm*/
            oa.mainodosize = json.mainodosize
            mainodosize.value = json.mainodosize
          }
          if (json.averages == undefined) {averages.checked = false} else {
            oa.averages = json.averages
            averages.checked = json.averages
          }
          if (json.graph == undefined) { graphEnab.checked = true } else {
            oa.graph = json.graph
            graphEnab.checked = json.graph
          }
          if (json.banner == undefined) { banner.checked = true } else {
            oa.banner = json.banner
            banner.checked = json.banner
          }
          if (json.image == undefined) { image.checked = true } else {
            oa.image = json.image
            image.checked = json.image
          }
          if (json.name == undefined) { uname.checked = true } else {
            oa.name = json.name
            uname.checked = json.name
          }
          if (json.icon == undefined) { uicon.checked = true } else {
            oa.icon = json.icon
            uicon.checked = json.icon
          }
          if (json.roundimage == undefined) { roundimage.checked = true } else {
            oa.roundimage = json.roundimage
            roundimage.checked = json.roundimage
          }
          console.log(oa,json)
          localStorage.setItem('UCmSp4bDxS9R0jpeZEvkut2gyoutube-subscriber-counterpage', JSON.stringify(oa))
        }
        
        
        
        
