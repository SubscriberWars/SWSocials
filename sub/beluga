        window.setodo = function (body) {
          const json = JSON.parse(body)
          console.log(json.down, json.up, json.speed, json.type)

          var oa = {}
          if (!json.down || json.down == "#010101") { } else {
            lestylehere.innerHTML += ".odometer-animating-down {color:" + json.down + " !important;}"
            oa.down = json.down
            odometerdowncolor.value = json.down
          }
          if (!json.up || json.up == "#010101") { } else {
            lestylehere.innerHTML += ".odometer-animating-up {color:" + json.up + " !important;}"
            oa.up = json.up
            odometerupcolor.value = json.up
          }
          if (!json.speed) { } else {
            
            lestylehere.innerHTML += ".odometer.odometer-auto-theme.odometer-animating-down .odometer-ribbon-inner,.odometer.odometer-theme-minimal.odometer-animating-down .odometer-ribbon-inner {-webkit-transition: -webkit-transform " + json.speed + "s !important;-moz-transition: -moz-transform " + json.speed + "s !important;-ms-transition: -ms-transform " + json.speed + "s !important;-o-transition: -o-transform " + json.speed + "s !important;transition: transform " + json.speed + "s !important; }"
            lestylehere.innerHTML += ".odometer.odometer-auto-theme.odometer-animating-up .odometer-ribbon-inner,.odometer.odometer-theme-minimal.odometer-animating-up .odometer-ribbon-inner {-webkit-transition: -webkit-transform " + json.speed + "s !important;-moz-transition: -moz-transform " + json.speed + "s !important;-ms-transition: -ms-transform " + json.speed + "s !important;-o-transition: -o-transform " + json.speed + "s !important;transition: transform " + json.speed + "s !important; }"

            oa.speed = json.speed
            odometerspeed.value = json.speed
          }
          if (!json.type) { } else {
            console.log(json.type)
            //holy shit this sucks
            if(json.type == "minimal") {
              document.querySelector('pain').innerHTML = ""
              document.querySelector('pain').innerHTML = `<script src="/odometer-min.js"><\/script> <link rel="stylesheet" href="/odometer-min.css">`.replace(/!%/g, "<").replace(/%!/g, ">")
            }
            if(json.type == "default") {
              document.querySelector('pain').innerHTML = ""
              document.querySelector('pain').innerHTML = `<script src="/odometer.js"><\/script> <link rel="stylesheet" href="/odometer.css">`.replace(/!%/g, "<").replace(/%!/g, ">")
            }
            oa.type = json.type
            odoslc.value = json.type
          }

          localStorage.setItem('UCmSp4bDxS9R0jpeZEvkut2gyoutube-subscriber-counterodo', JSON.stringify(oa))
        }
