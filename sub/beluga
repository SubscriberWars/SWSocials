        window.avrgs = function (c) {
          console.log(c);
          //then we check that is the averages enabled.
        const pgg = JSON.parse(localStorage.getItem('UCmSp4bDxS9R0jpeZEvkut2gyoutube-subscriber-counterpage')||"{}"); //i think there is an more efficient way to do this. oh well.
        if(pgg.averages == true) {
          window.avgs.push(parseFloat(c))
          if(window.avgs.length > (60/window.updtint)) {
            window.avgs.shift();//remove first element
          }
          if((window.avgstype||false) == false) {//average type 1
            //og avg type
            let count = 0;
            for(var i = 1; i < window.avgs.length; i++) {
              count+=(window.avgs[i]-window.avgs[i-1])
            }
            const jaa = count/window.avgs.length;
            const averagebox = document.querySelectorAll('.useravgs div div.odometer')
            averagebox[0].innerHTML = jaa/window.updtint //1 sec
            averagebox[1].innerHTML = Math.floor((jaa/window.updtint)*60) //1 min
            averagebox[2].innerHTML = Math.floor((jaa/window.updtint)*3600) //1 hour
            return console.log(jaa, "avg type 1")

          }
        } else {
          window.avgs = []
        }
        }
