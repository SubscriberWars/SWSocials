    <script>
      const tabs = document.querySelectorAll('.tab')
      window.showTab = (id) => {
        for (var i = 0; i < tabs.length; i++) {
          if (i == id) {
            tabs[i].style.display = ""
          } else {
            tabs[i].style.display = "none"
          }
        }
      }
    </script>
