        function gengraph() {
          const onex = JSON.parse(localStorage.getItem('UCmSp4bDxS9R0jpeZEvkut2gyoutube-subscriber-countergraph')||"{}")
          const twox = JSON.parse(localStorage.getItem('globalgraph')||"{}")
          const one = JSON.parse(localStorage.getItem('UCmSp4bDxS9R0jpeZEvkut2gyoutube-subscriber-counterpage')||"{}")
          const two = JSON.parse(localStorage.getItem('globalpage')||"{}")
        const pgg = {...two, ...one };
        const json = {...twox, ...onex };
          console.log(pgg, json, "pgg, json")
