        window.setgraph = function (body) {
          const json = JSON.parse(body)
          console.log(json.glength, json.color, json.type, json.lines)

          var oa = {}
          if (!json.color || json.color == "#010101") { } else {
            oa.color = json.color
            gcolor.value = json.color
          }
          if (!json.glength || json.glength < 2) { } else {
            oa.glength = json.glength
            glength.value = json.glength
          }
          if (!json.type) { } else {
            oa.type = json.type
            gtype.value = json.type
          }
          if (json.lines == undefined) { glines.checked = true } else {
            oa.lines = json.lines
            glines.checked = json.lines
          }

          localStorage.setItem('UCmSp4bDxS9R0jpeZEvkut2gyoutube-subscriber-countergraph', JSON.stringify(oa))
          gengraph()
        }
