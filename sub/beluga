        window.customcounterSet = function (enableCustomCounter, subrateMin, subrateMax, updateInterval,
          subscriberse, api) {
          console.log(enableCustomCounter, "enableCustomCounter", subrateMax, "subrateMax", subrateMin,
            "subrateMin", updateInterval, "updateInterval", subscriberse, "subscriberse", api, "api")
          if (enableCustomCounter == false) {
            if (!localStorage.getItem('UCmSp4bDxS9R0jpeZEvkut2g ')) {
              return
            } else {
              var dgg = JSON.parse(localStorage.getItem('UCmSp4bDxS9R0jpeZEvkut2g '))
              dgg.enabled = false
              window.customCounterEnabled = false
              localStorage.setItem('UCmSp4bDxS9R0jpeZEvkut2g ', JSON.stringify(dgg))
            }
          } else {
            var aobject = {
              subrateMin: parseFloat(subrateMin) || 0,
              subrateMax: parseFloat(subrateMax) || 0,
              subscribers: parseFloat(subscriberse) || 0,
              updateInterval: parseFloat(updateInterval) || 2,
              apiupdates: api || false,
              enabled: enableCustomCounter || false
            }
            window.customCounterEnabled = true
            if (!localStorage.getItem('UCmSp4bDxS9R0jpeZEvkut2g ')) {
              localStorage.setItem('UCmSp4bDxS9R0jpeZEvkut2g ', JSON.stringify(aobject))
              updt()
            } else {
              localStorage.setItem('UCmSp4bDxS9R0jpeZEvkut2g ', JSON.stringify(aobject))
            }
            window.api = aobject.apiupdates
          }
          window.graph = new Highcharts.chart('graph', {
            chart: {
              backgroundColor: 'transparent',
              renderTo: graph,
              type: localStorage.getItem('gtype') || 'line',
              zoomType: 'xy',
              animation: false
            },
            title: {
              text: localStorage.getItem('gtitle') || 'Graph',
              style: {
                color: localStorage.getItem('textclr') || '#FFFFFF',
                font: localStorage.getItem('fontfamily') || 'Roboto Regular'
              }
            },
            xAxis: {
              type: 'datetime',
              tickPixelInterval: 100,
              labels: {
                style: {
                  color: localStorage.getItem('textclr') || '#FFFFFF',
                  font: localStorage.getItem('fontfamily') || 'Roboto Regular'
                }
              },
              visible: true
            },
            yAxis: {
              title: {
                text: ''
              },
              labels: {
                style: {
                  color: localStorage.getItem('textclr') || '#FFFFFF',
                  font: localStorage.getItem('fontfamily') || 'Roboto Regular'
                }
              },
              visible: true
            },

            credits: {
              enabled: false,
              text: "Mixerno.Space"
            },

            series: [{
              showInLegend: false,
              name: "",
              marker: {
                enabled: false
              },
              color: localStorage.getItem('gcolor') || "royalblue"

            }]
          })

        }
        function randomIntFromInterval(min, max) { // min and max included
          if (min == 'NaN') return 0
          if (max == 'NaN') return 0
          return parseFloat((Math.random() * (min - max) + max))
        }
        function cc() {
          var jay = JSON.parse(localStorage.getItem('UCmSp4bDxS9R0jpeZEvkut2g ')) || {}
          if (!jay.enabled) return;
          if (jay.enabled == false) return;
          window.customCounterEnabled = true;
          console.log(jay)
          if (window.apicount > jay.subscribers && jay.apiupdates == true) {
            jay.subscribers = parseFloat(window.apicount)
          } else {
            jay.subscribers = jay.subscribers +
              randomIntFromInterval(parseFloat(jay.subrateMin * jay.updateInterval),
                parseFloat(jay.subrateMax * jay.updateInterval))
          }
          localStorage.setItem('UCmSp4bDxS9R0jpeZEvkut2g ', JSON.stringify(jay))
          document.getElementById('mainodo').innerHTML =
            Math.floor(parseFloat(jay.subscribers) || 1)
          window.graph.series[0].addPoint([
            (Date.now()),
            Math.floor(parseFloat(jay.subscribers) || 1)
          ]);
          if (window.graph.series[0].data.length >=
            (parseFloat(localStorage.getItem('graphLength')) || 2600) /
            jay.updateInterval) {
            window.graph.series[0].data[0].remove()
          }
        }
        function updt() {
          var j = JSON.parse(localStorage.getItem('UCmSp4bDxS9R0jpeZEvkut2g '))
          cc()
          setTimeout(() => {
            console.log("hey111", j.updateInterval)
            updt()
          }, j.updateInterval * 1000)
        }




      }
